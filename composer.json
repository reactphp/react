    "keywords": ["event-loop", "reactor"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "evenement/evenement": "dev-master",
        "guzzle/guzzle": "2.5.*",
        "silex/silex": "1.0.*"
